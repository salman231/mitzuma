<?xml version="1.0"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/mstore/storeConfig/:cartId" method="GET">
        <service class="Mstore\Checkout\Api\StoreConfigInterface" method="getStoreConfigsFromCart"/>
        <resources>
            <resource ref="anonymous" />
        </resources>
    </route>
</routes>