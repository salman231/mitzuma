<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <mobikul>
            <configuration>
                <apiusername><![CDATA[mobikul]]></apiusername>
                <apikey><![CDATA[mobikul123]]></apikey>
                <pagesize><![CDATA[10]]></pagesize>
                <featuredproduct><![CDATA[1]]></featuredproduct>
                <splash_screen><![CDATA[splashscreen.png]]></splash_screen>
            </configuration>
            <searchsuggestion>
                <displaytag><![CDATA[1]]></displaytag>
                <tagcount>5</tagcount>
                <displayproduct><![CDATA[1]]></displayproduct>
                <productcount>5</productcount>
            </searchsuggestion>
            <appdownload>
                <allowAndroid><![CDATA[0]]></allowAndroid>
                <allowiOS><![CDATA[0]]></allowiOS>
                <enabletoplink><![CDATA[0]]></enabletoplink>
                <enablebottomlink><![CDATA[0]]></enablebottomlink>
                <downloadlinktheme><![CDATA[mk-lightTheme]]></downloadlinktheme>
            </appdownload>
            <themeConfig>
                <themeType>0</themeType>
            </themeConfig>
            <tab_category_view>
                <tab_category_view>0</tab_category_view>
            </tab_category_view>
            <app_logo>
                <launcher_icon_type>1</launcher_icon_type>
            </app_logo>
            <color_pallet>
                <app_button_color>#1a37ff</app_button_color>
                <app_theme_color>#ff4aab</app_theme_color>
                <button_text_color>#223200</button_text_color>
                <app_background_color>#0086b1</app_background_color>
            </color_pallet>
            <walkthrough>
                <walkthrough_version>1.0</walkthrough_version>
            </walkthrough>
        </mobikul>
    </default>
